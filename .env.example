# AIrWAVE Environment Variables Template
# Copy this file to .env.local for development

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret
JWT_SECRET=your-jwt-secret

# External APIs
CREATOMATE_API_KEY=your-creatomate-key
CREATOMATE_WEBHOOK_SECRET=your-webhook-secret
OPENAI_API_KEY=your-openai-key
ELEVENLABS_API_KEY=your-elevenlabs-key
RUNWAY_API_KEY=your-runway-key

# Email Service (Resend)
RESEND_API_KEY=re_xxxxxxxxxxxxx

# Error Tracking (Sentry)
NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx
SENTRY_AUTH_TOKEN=your-sentry-auth-token
SENTRY_ORG=your-org
SENTRY_PROJECT=airwave

# Storage (AWS S3)
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=airwave-production-assets
CDN_DOMAIN=cdn.airwave.com

# Rate Limiting (Upstash Redis)
UPSTASH_REDIS_URL=https://xxx.upstash.io
UPSTASH_REDIS_TOKEN=your-redis-token

# Job Queue (Redis)
REDIS_URL=redis://localhost:6379

# Analytics
NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
NEXT_PUBLIC_ENABLE_ANALYTICS=false # Set to true in production

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=AIrWAVE
NEXT_PUBLIC_APP_VERSION=1.0.0

# Worker Configuration
RENDER_WORKER_CONCURRENCY=3
EMAIL_WORKER_CONCURRENCY=5
WEBHOOK_WORKER_CONCURRENCY=10
ANALYTICS_WORKER_CONCURRENCY=10

# Feature Flags
ENABLE_PROTOTYPE_MODE=false
ENABLE_DEBUG_LOGGING=false
