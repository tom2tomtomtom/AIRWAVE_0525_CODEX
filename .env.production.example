# Production Environment Variables
# These should be set in your hosting provider (Netlify/Vercel)

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-production-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-production-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-production-service-role-key

# Authentication
NEXTAUTH_URL=https://app.airwave.com
NEXTAUTH_SECRET=generate-strong-secret-here
JWT_SECRET=generate-strong-jwt-secret

# External APIs
CREATOMATE_API_KEY=your-production-creatomate-key
CREATOMATE_WEBHOOK_SECRET=your-production-webhook-secret
OPENAI_API_KEY=your-production-openai-key
ELEVENLABS_API_KEY=your-production-elevenlabs-key
RUNWAY_API_KEY=your-production-runway-key

# Email Service
RESEND_API_KEY=re_production_key

# Error Tracking
NEXT_PUBLIC_SENTRY_DSN=https://production@sentry.io/xxx
SENTRY_AUTH_TOKEN=your-production-sentry-token
SENTRY_ORG=airwave
SENTRY_PROJECT=airwave-production
NEXT_PUBLIC_SENTRY_RELEASE=1.0.0

# Storage
AWS_ACCESS_KEY_ID=production-access-key
AWS_SECRET_ACCESS_KEY=production-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=airwave-production-assets
CDN_DOMAIN=cdn.airwave.com

# Rate Limiting
UPSTASH_REDIS_URL=https://production.upstash.io
UPSTASH_REDIS_TOKEN=production-redis-token

# Job Queue
REDIS_URL=redis://production-redis:6379

# Analytics
NEXT_PUBLIC_MIXPANEL_TOKEN=production-mixpanel-token
NEXT_PUBLIC_GA_ID=G-PRODUCTION
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Application
NEXT_PUBLIC_APP_URL=https://app.airwave.com
NEXT_PUBLIC_APP_NAME=AIrWAVE
NEXT_PUBLIC_APP_VERSION=1.0.0

# Workers
RENDER_WORKER_CONCURRENCY=10
EMAIL_WORKER_CONCURRENCY=20
WEBHOOK_WORKER_CONCURRENCY=50
ANALYTICS_WORKER_CONCURRENCY=20

# Feature Flags
ENABLE_PROTOTYPE_MODE=false
ENABLE_DEBUG_LOGGING=false
